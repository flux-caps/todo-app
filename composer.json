{
  "name": "flux-cap/todo-app",
  "description": "Component of FluxCapacitor developed by fluxlabs",
  "version": "0.5.0",
  "type": "flux-app",
  "keywords": [
    "flux-caps",
    "todo-app",
    "fluxlabs"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "connect@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "connect@fluxlabs.ch"
  },
  "require": {
    "flux-eco/api-gateway-event-sourced-app": "=1.1.1",
    "flux-eco/projection": "=1.4.0",
    "flux-eco/aggregate-root": "=1.0.2",
    "flux-eco/global-stream": "=1.0.0",
    "flux-eco/storage": "=1.1.0",
    "flux-eco/value-object": "=1.0.0",
    "flux-eco/ui-transformer": "=1.0.0",
    "flux-eco/json-schema-assertion": ">=0.0.1",
    "flux-eco/open-api-client": "=1.0.0",
    "flux-eco/json-schema-instance": "=1.0.0",
    "flux-eco/json-schema-document": "=1.0.1",
    "flux-eco/shell-executor": ">=0.0.1",
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-yaml": "*",
    "composer-runtime-api": ">=2.1"
  },
  "require-dev": {
    "composer/composer": ">=2.0",
    "swoole/ide-helper": ">=4.6"
  },
  "autoload": {
    "psr-4": {
      "FluxCap\\TodoApp\\": [
        "src/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
